{
  "name": "android-semantic-release",
  "version": "1.1.0",
  "private": false,
  "description": "GitHub action for bumping Android repos using semantic releases.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "clean": "rimraf lib dist",
    "format": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map",
    "prepare": "husky install",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oflynned/Android-Semantic-Release.git"
  },
  "keywords": [
    "android",
    "semantic",
    "version",
    "release"
  ],
  "author": "oflynned",
  "license": "MIT",
  "dependencies": {
    "@octokit/core": "^3.5.1",
    "actions-toolkit": "^6.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.7",
    "@typescript-eslint/parser": "^4.28.5",
    "@vercel/ncc": "^0.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-github": "^4.1.5",
    "eslint-plugin-jest": "^24.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "jest-mock-extended": "^1.0.18",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.1.1",
    "npm-check-updates": "^11.8.3",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.3.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": "10 || 12 || 14",
    "npm": ">=6",
    "yarn": "please-use-npm"
  }
}
